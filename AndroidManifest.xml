            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.talk.TalkActivity"
            android:label="talk" android:noHistory="true"/>
        <activity android:name=".ui.history.HistoryActivity"
            android:label="history" android:noHistory="true" />
        <activity android:name=".ui.talk.EmptyActivity"
            android:label="empty" android:noHistory="true"/>
        <activity android:name=".ui.login.ConfirmActivity"
            android:label="confirm" android:noHistory="true"/>
        <activity android:name=".ui.login.LoginActivity"
            android:label="login" android:noHistory="true"/>
        <activity android:name=".ui.login.DetailsActivity"
            android:label="details" android:noHistory="true"/>
    </application>
</manifest>
